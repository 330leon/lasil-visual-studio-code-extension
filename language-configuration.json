{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
      { "open": "\"", "close": "\"" },
      { "open": "(", "close": ")" },
      { "open": "[", "close": "]" },
      { "open": "{", "close": "}" }
    ],
    "surroundingPairs": [
      { "open": "\"", "close": "\"" },
      { "open": "(", "close": ")" },
      { "open": "[", "close": "]" },
      { "open": "{", "close": "}" }
    ],
    "folding": {
      "markers": {
        "start": "^\\s*define (class|method)\\b",
        "end": "^\\s*define (class_end|method_end)\\b"
      }
    }
  }